{
    "name": "phpwatch/composer-mini-autoload",
    "type": "composer-plugin",
    "require": {
        "php": "^7.4",
        "composer-plugin-api": "^1.0 || ^2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.1",
        "composer/composer": "^1.7 || ^2.0"
    },
    "extra": {
        "class": "PHPWatch\\ComposerAutoloadMin\\Plugin"
    },
    "autoload": {
        "psr-4": {
            "PHPWatch\\ComposerAutoloadMin\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "PHPWatch\\ComposerAutoloadMin\\Tests": "tests/"
        }
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Ayesh Karunaratne",
            "email": "ayesh@ayesh.me"
        }
    ]
}
